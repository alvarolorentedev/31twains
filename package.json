{
  "name": "31twains-server",
  "version": "0.0.1",
  "description": "server side for 31twains",
  "main": "src/server.ts",
  "scripts": {
    "test": "USERNAME=test PASSWORD=test jest",
    "test:dev": "USERNAME=test PASSWORD=test jest --watchAll",
    "test:cov": "USERNAME=test PASSWORD=test jest --coverage && cat ./coverage/lcov.info",
    "start:local": "NODE_ENV=development ts-node .",
    "start": "NODE_ENV=production ts-node .",
    "start:dev": "NODE_ENV=development USERNAME=test PASSWORD=test ts-node-dev --poll .",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "migrate:local:init": "touch local.sqlite && NODE_ENV=development db-migrate up",
    "migrate:local:up": "db-migrate up -e development",
    "migrate:remote:up": "db-migrate up -e production",
    "migrate:local:down": "db-migrate down -e development",
    "migrate:remote:down": "db-migrate down -e production",
    "migrate:create": "db-migrate create"
  },
  "pre-commit": [
    "lint",
    "test"
  ],
  "repository": "git@github.com:kanekotic/31twains.git",
  "author": "kanekotic <alvarojosepl@gmail.com>",
  "private": true,
  "dependencies": {
    "@types/cors": "2.8.10",
    "@types/express": "4.17.11",
    "@types/node": "14.14.41",
    "@types/uuid": "8.3.0",
    "@typescript-eslint/parser": "4.22.0",
    "cors": "2.8.5",
    "express": "4.17.1",
    "helmet": "4.5.0",
    "knex": "0.95.4",
    "morgan": "1.10.0",
    "pg": "8.6.0",
    "ts-node": "9.1.1",
    "typescript": "4.2.4",
    "uuid": "8.3.2",
    "winston": "3.3.3",
    "winston-loggly-bulk": "3.2.1"
  },
  "devDependencies": {
    "@types/jest": "26.0.22",
    "@types/supertest": "2.0.11",
    "@typescript-eslint/eslint-plugin": "4.22.0",
    "db-migrate": "0.11.12",
    "db-migrate-pg": "1.2.2",
    "db-migrate-sqlite3": "0.4.0",
    "eslint": "7.24.0",
    "eslint-config-prettier": "8.2.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-prettier": "3.4.0",
    "faker": "5.5.3",
    "jest": "26.6.3",
    "nodemon": "2.0.7",
    "pre-commit": "1.2.2",
    "prettier": "2.2.1",
    "sqlite3": "5.0.2",
    "supertest": "6.1.3",
    "ts-jest": "26.5.5",
    "ts-node-dev": "1.1.6"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  }
}
